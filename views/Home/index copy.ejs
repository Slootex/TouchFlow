            <div class="h-1">
                <div class="float-left">
                    <p class="pt-10 text-3xl font-medium"><%= dateTime %>,</p>
                    <p class="text-3xl font-light font-bold text-zinc-500">Lucas</p>
                </div>
                <div onclick="getWeatherModal()" class="float-right mt-10 flex">
                    <%- include("../components/weather/smallMenu.ejs") %>
                </div>
            </div>
    
            <div class="">
                <p style="font-size: 7rem;" class="m-auto text-center" id="current-time"><%= hr %>:<%= min %><span class=" ml-1 text-4xl"><%= seconds %></span></p>
            </div>
            
            <div class="px-4 py-4 bg-zinc-800 rounded-2xl grid grid-cols-3 gap-4 mt-4">
                <p class="text-3xl white py-2 col-span-3">Addons</p>
