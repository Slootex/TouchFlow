<body class=" m-auto fontstyle h-screen overflow-hidden scrollbar" id="body" style=" background-color: #222222; color: #ffffff">
    <% let today = new Date().getDate(); %>
    <% let hour = new Date().getHours(); %>
    
    <div id="calender-fullscreen-div">

    </div>
    <div id="weather-fullscreen-div" class="overflow-auto w-full">

    </div>
   
   

