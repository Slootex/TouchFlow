            <div class="absolute" style="max-width: 150%"> 
                <div class="m-auto flex">
                    <% for (let i = 0; i <  weather.daily.time.length; i++) { %>
                        <% let currentDate  = weather.daily.time[i]; %>
                        <% let date         = new Date(currentDate); %>
                        <% let today        = new Date(); %>
                        <% let temp_max     = weather.daily.temperature_2m_max[i] %>
                        <% let uv           = weather.daily.uv_index_max[i] %>
                        <% let rain         = weather.daily.precipitation_probability_mean[i] %>
                        <% let wind         = weather.daily.wind_speed_10m_max[i] %>

                        <% if(date.getDay() == today.getDay()) { continue; } %>

                        <div class=" rounded-2xl text-white  mt-6  w-72 ml-4" style="  background-color: #464646;"   ><p class="text-center mb-5 mt-3">
                            <%= date.toLocaleString('de-de', {  weekday: 'long' }); %>
                        </p>
                            <div class="">
                                <div class="m-auto">
                                    <div class="flex m-auto ">
                                        
